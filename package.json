  "description": "网易云音乐web-app with pwa",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "commit": "git-cz",
    "build:client": "webpack --watch --mode=production --config 'webpack.client.js'",
    "build:server": "webpack --build=server --watch --mode=production --config 'webpack.server.js'",
    "start:client": "NODE_ENV=development webpack-dev-server --host 0.0.0.0 --mode=development  --hot  --config 'webpack.client.js'",
    "build-client": "webpack --mode=production --config 'webpack.client.js'",
    "build-server": "webpack --build=server --mode=production --config 'webpack.server.js'"
  },
