    "hooks": {
      "commit-msg": "lint-staged"
    }
  },
  "lint-staged": {
    "{src,test}/**/*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
